{
  "extends": ["airbnb"],
  "rules": {
    "global-require": 1,
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "object-curly-spacing": [2, "never"],
    "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx"]}],
    "react/jsx-props-no-spreading": 0,
    "react/state-in-constructor": 0
  },
  "globals": {
    "document": true,
    "window": true,
    "it": true,
    "expect": true
  },
  "parser": "babel-eslint"
}
